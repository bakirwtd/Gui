
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Main", "Ocean")
local Tab = Window:NewTab("Player")
local Section = Tab:NewSection("GodMode")
Section:NewButton("Godmode", "Clicking This Will Give You Godmode. Reset To Disable", function()
   print("godmode") 
end)


local Tab2 = Window:NewTab("Free Guns")
local Section2 = Tab2:NewSection("Guns:")
Section2:NewButton("Get Minigun", "Gives You Minigun, WILL CHARGE MONEY!", function()
    print("gun")
end)
Section2:NewButton("Get Death Awp", "Gives You Death Awp, WILL CHARGE MONEY!", function()
    print("gun")
end)
Section2:NewButton("Get Hell Fire Shotguns", "Gives You HellFire, WILL CHARGE MONEY!", function()
    print("gun")
end)
Section2:NewButton("Get M4A1", "Gives You M4A1, WILL CHARGE MONEY!", function()
    print("gun")
end)
Section2:NewButton("Get Mac-10", "Gives You Mac10, WILL CHARGE 10K!", function()
    print("gun")
end)

local Tab3 = Window:NewTab("Admin Stuff")
local Section3 = Tab3:NewSection("Section Name")
Section3:NewButton("SuperKnife", "When Clicked, Equip A Knife And Click E, To Disable Unequip The Knife.", function()
    print("Knife Admin Power On")
end)
